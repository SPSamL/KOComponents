{
  "compileOnSave": false,
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": false,
    "removeComments": false,
    "target": "es5",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "amd",
    "sourceMap": false,
    "declaration": false,
    "moduleResolution": "node"
  },
  "exclude": [
    "wwwroot",
    "node_modules"
  ]
}
